<?xml version="1.0" encoding="UTF-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android"  
    android:shape="line" >  
      
    <!--线宽为dashWith，线之间空隙dashGap，dashGap=0dp时，是实线 -->  
    <stroke  
        android:dashGap="1dp"
        android:dashWidth="5dp"  
        android:width="0.1dp"
        android:color="#b4b4b4" />
      
    <!-- 虚线高度 -->  
    <size android:height="0.1dp" />    
      
</shape> 